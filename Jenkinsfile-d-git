pipeline{
    agent any
	environment{
        PATH = "/usr/share/maven/bin:$PATH"
    }
    
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'github', url: 'https://github.com/vidyasagarkolusu/myweb'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
               
            }
        }
       
    }
}
